{
  "name": "whatson-v2",
  "version": "2.3.0",
  "description": "Nodejs remake of xjarlie/tv-recommend",
  "main": "server.ts",
  "scripts": {
    "dev": "nodemon -e ts,js,ejs,html server.ts",
    "build": "npm run ts-build && npm run docker-build",
    "ts-build": "npx tsc",
    "docker-build": "docker build . -t xjarlie/whatson:beta-ts && docker push xjarlie/whatson:beta-ts",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xjarlie/whatson-v2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xjarlie/whatson-v2/issues"
  },
  "homepage": "https://github.com/xjarlie/whatson-v2#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "web-push": "^3.4.5",
    "x-jsondb": "^1.0.5"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.15",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/web-push": "^3.3.2",
    "nodemon": "^2.0.15",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
