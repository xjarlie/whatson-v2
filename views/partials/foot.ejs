<script src="/public/materialize.min.js"></script>
<script src="/public/pwa.js"></script>
<script>
    function refresh() {
        window.location.reload();
        return false;
    }

    function reload() {
        refresh();
    }

    function redirect(url) {
        window.location.href = url;
        return false;
    }

    function goBack() {
        const params = new URLSearchParams(window.location.search);
        if (params.get('referrer')) {
            redirect(`/app${params.get('referrer')}`);
        } else {
            redirect('/app');
        }
    }
</script>